{
  "name": "@nyxb/eslint-config",
  "type": "module",
  "version": "1.0.4",
  "packageManager": "pnpm@8.10.2",
  "description": "Nyxb's ESLint config",
  "author": "Dennis Ollhoff <contact@nyxb.email> (https://github.com/nyxb/)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/nyxb",
  "homepage": "https://github.com/nyxb/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "tsup --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "pnpm run stub && eslint .",
    "prepack": "nyxr build",
    "release": "bumping && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^14.0.0",
    "@nyxb/eslint-define-config": "^1.24.1-1",
    "@stylistic/eslint-plugin": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-flat-gitignore": "^0.1.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-nyxb": "^1.0.1",
    "eslint-plugin-perfectionist": "^2.2.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vitest": "^0.3.9",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-plugin-yml": "^1.10.0",
    "execa": "^8.0.1",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "vue-eslint-parser": "^9.3.2",
    "yaml-eslint-parser": "^1.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@nyxb/eslint-config": "workspace:*",
    "@nyxb/nyxi": "^0.0.31",
    "@stylistic/eslint-plugin-migrate": "^1.1.0",
    "@types/eslint": "^8.44.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.9.0",
    "@types/prompts": "^2.4.8",
    "@types/yargs": "^17.0.31",
    "bumping": "^9.2.1",
    "eslint": "^8.53.0",
    "eslint-flat-config-viewer": "^0.1.1",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "lint-staged": "^15.0.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
